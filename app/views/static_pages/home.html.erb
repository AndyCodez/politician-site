<div class="img_carousel">
	<!-- Sliding img carousel goes here-->
	<div class="orbit" role="region" aria-label="Favorite Space Pictures" data-orbit>
		<ul class="orbit-container">
			<button class="orbit-previous"><span class="show-for-sr">Previous Slide</span>&#9664;&#xFE0E;</button>
			<button class="orbit-next"><span class="show-for-sr">Next Slide</span>&#9654;&#xFE0E;</button>
			<li class="is-active orbit-slide">
				<%= image_tag('orbit/01.jpg', class: "orbit-image") %>
			<!-- <img class="orbit-image" src="assets/orbit/01.jpg" alt="01"> -->
				<figcaption class="orbit-caption">Hon. John Doe at the Church Fundraiser</figcaption>
			</li>
			<li class="orbit-slide">
				<%= image_tag('orbit/02.jpg', class: "orbit-image") %>
				<!-- <img class="orbit-image" src="assets/orbit/02.jpg" alt="02"> -->
				<figcaption class="orbit-caption">During the meeting with Russian Delegates.</figcaption>
			</li>
			<li class="orbit-slide">
				<%= image_tag('orbit/03.jpg', class: "orbit-image") %>
				<!-- <img class="orbit-image" src="assets/orbit/03.jpg" alt="03"> -->
				<figcaption class="orbit-caption">Opening Primary Schools in Rural Kenya</figcaption>
			</li>
			<li class="orbit-slide">
				<%= image_tag('orbit/04.jpg', class: "orbit-image") %>
				<!-- <img class="orbit-image" src="assets/orbit/04.jpg" alt="04"> -->
				<figcaption class="orbit-caption">Visitin A Children's Home</figcaption>
			</li>
		</ul>
		<nav class="orbit-bullets">
			<button class="is-active" data-slide="0"><span class="show-for-sr">First slide details.</span><span class="show-for-sr">Current Slide</span></button>
			<button data-slide="1"><span class="show-for-sr">Second slide details.</span></button>
			<button data-slide="2"><span class="show-for-sr">Third slide details.</span></button>
			<button data-slide="3"><span class="show-for-sr">Fourth slide details.</span></button>
		</nav>
	</div>
</div>
<div class="content_home">
	<div id="vision"></div>
	<div id="press_coverage"></div>
	<div id="lets_get_social"></div>
</div>